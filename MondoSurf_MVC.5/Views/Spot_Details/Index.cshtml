@model IEnumerable<MondoSurf_MVC._5.Models.Spot_Details>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Spot_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.County_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Street_Address)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Spot_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.County_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Latitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Longitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Street_Address)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Spot_Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Spot_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Spot_Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
